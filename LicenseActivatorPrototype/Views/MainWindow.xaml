<Window
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="LicenseActivatorPrototype"
    WindowStartupLocation="CenterScreen"
    SizeToContent="WidthAndHeight"    
    Width="400"
    Height="200">
    <Grid Margin="10,10,10,10">
        <StackPanel Orientation="Vertical" Gap="5">
            <Button Content="Register License" Command="{Binding RegisterLicenseCommand}" />
            <Button Content="Unregister License" Command="{Binding UnRegisterLicenseCommand}"/>
        </StackPanel>
    </Grid>
</Window>